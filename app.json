{
  "expo": {
    "name": "Axia Africa C9",
    "slug": "axia-africa-react-native-c9",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "axiaafricareactnativec9",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "icon": {
        "light": "./assets/splash/ios-light-icon.png",
        "dark": "./assets/splash/ios-dark-icon.png",
        "tinted": "./assets/splash/ios-tinted-icon.png"
      },
      "supportsTablet": true,
      "config": {
        "usesNonExemptEncryption": false
      },
      "bundleIdentifier": "com.farminnovation.axiaafricareactnativec9"
    },
    "android": {
      "permissions": [
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.ACCESS_MEDIA_LOCATION",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "adaptiveIcon": {
        "backgroundColor": "#FCA1A1",
        "foregroundImage": "./assets/splash/android-adaptive-icon.png",
        "monochromeImage": "./assets/splash/android-adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.farminnovation.axiaafricareactnativec9"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-media-library",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) to access your photos.",
          "savePhotosPermission": "Allow $(PRODUCT_NAME) to save photos.",
          "isAccessMediaLocationEnabled": true
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera",
          "microphonePermission": "Allow $(PRODUCT_NAME) to access your microphone",
          "recordAudioAndroid": true
        }
      ],
      "expo-video",
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/splash/splash-light-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#FCA1A1",
          "dark": {
            "backgroundColor": "#0C0C0C",
            "image": "./assets/splash/splash-dark-icon.png"
          }
        }
      ],
      [
        "expo-secure-store",
        {
          "configureAndroidBackup": true,
          "faceIDPermission": "Allow AXIA AFRICA to access your Face ID biometric data."
        }
      ],
      "expo-video"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "42f60e04-92b2-4743-a530-176b35d4163e"
      }
    },
    "owner": "axia-africa"
  }
}
